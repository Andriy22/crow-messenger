<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Client.LogInPage"
             xmlns:mdc="clr-namespace:Material.Components.Maui;assembly=Material.Components.Maui">
    <VerticalStackLayout>

        <mdc:Label Text="LOG IN" Margin="5"></mdc:Label>
        <mdc:TextField LabelText="UserName" SupportingText="" Margin="5"></mdc:TextField>
        <mdc:TextField LabelText="Password" SupportingText="" Margin="5"></mdc:TextField>
        <HorizontalStackLayout>
            <mdc:Button Text="Log In" Margin="5"></mdc:Button>
            <mdc:Button Text="Registration" Margin="5" Clicked="GoToRegistration"></mdc:Button>
        </HorizontalStackLayout>
    </VerticalStackLayout>
</ContentPage>